    <enable>1</enable>
    <sx>6</sx>
    <sy>58</sy>
    <height>64</height>
    <width>116</width>
    <priority>13</priority>
    <cbtime>3000</cbtime>
    <font>0</font>
    <format>140</format>
    <frequency>500</frequency>
  </update>

  <!-- roaming -->
  <roaming>
    <enable>1</enable>
    <sx>6</sx>
    <sy>50</sy>
    <height>46</height>
    <width>116</width>
    <priority>2</priority>
    <font>2</font>
