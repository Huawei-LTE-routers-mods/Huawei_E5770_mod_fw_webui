  <!-- downloading -->
  <downloading>
    <enable>1</enable>
    <sx>6</sx>
    <sy>58</sy>
    <height>32</height>
    <width>116</width>
    <priority>13</priority>
    <font>0</font>
    <format>140</format>
    <frequency>1000</frequency>
  </downloading>

  <!-- download_progress -->
  <download_progress>
    <enable>1</enable>
    <sx>6</sx>
    <sy>90</sy>
    <height>32</height>
    <width>116</width>
    <priority>13</priority>
    <font>0</font>
    <format>140</format>
  </download_progress>

