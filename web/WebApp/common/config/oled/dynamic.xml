        <cbtime>10000</cbtime>
    </infomation>
    
    <!-- ID_DYNAMIC_IDLE_SSID -->
    <idlessid_cap>
        <enable>1</enable>
        <sx>45</sx>
        <sy>50</sy>
        <height>20</height>
        <width>75</width>  
        <font>2</font>
        <format>152</format>        
        <priority>0</priority>
    </idlessid_cap>	
	
    <!-- ID_DYNAMIC_IDLE_PWD-->
