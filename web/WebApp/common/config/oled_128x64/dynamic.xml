<?xml version="1.0" encoding="UTF-8"?>
<config>
    <!-- PLMN -->
    <plmn>
        <enable>1</enable>
        <sx>0</sx>
        <sy>86</sy>
        <height>22</height>
        <width>128</width>        
        <priority>1</priority>
    </plmn>

    <!-- apn -->
    <apn>
        <enable>0</enable>
        <sx>0</sx>
        <sy>80</sy>
        <height>22</height>
        <width>128</width>        
        <priority>0</priority>
    </apn>
	
    <!-- flux -->
    <flux>
        <enable>1</enable>
        <sx>0</sx>
        <sy>108</sy>
        <height>16</height>
        <width>128</width>
        <priority>0</priority>
        <font>2</font>
        <format>4</format>
    </flux>

    <!-- wifi-security_type -->
    <wifi_security>
        <enable>0</enable>
        <sx>18</sx>
        <sy>102</sy>
        <height>16</height>
        <width>92</width>        
        <priority>1</priority>
        <font>2</font>
        <format>4</format>
    </wifi_security>
	
    <!-- wifi-ssid -->
    <ssid>
        <enable>1</enable>
        <sx>39</sx>
        <sy>72</sy>
        <height>20</height>
        <width>89</width>        
        <priority>7</priority>
        <cbtime>10000</cbtime>
        <font>2</font>
        <format>8</format>
        <frequency>1000</frequency>
    </ssid>
	
    <!-- wifi-key -->
    <key>
        <enable>1</enable>
        <sx>39</sx>
        <sy>93</sy>
        <height>31</height>
        <width>89</width>        
        <priority>7</priority>
        <cbtime>10000</cbtime>
        <font>2</font>
        <format>8</format>
        <frequency>1000</frequency>
    </key>

	<!-- ID_DYNMIC_BATTERY_PENCENT -->
    <batt_percent_poweroff_charge>
        <enable>1</enable>
        <sx>8</sx>
        <sy>72</sy>
        <height>20</height>
        <width>124</width> 
        <font>0</font>  
        <format>4</format>       
        <priority>1</priority>
    </batt_percent_poweroff_charge>
	
	<!-- ID_DYNMIC_POWEROFF_EXCHARGE_PERCENT -->
    <batt_percent_poweroff_excharge_display>
        <enable>1</enable>
        <sx>46</sx>
        <sy>74</sy>
        <height>28</height>
        <width>54</width>
        <font>0</font>
        <format>4</format>
        <priority>1</priority>
    </batt_percent_poweroff_excharge_display>
    <ssid_cap>
        <enable>1</enable>
        <sx>1</sx>
        <sy>72</sy>
        <height>20</height>
        <width>37</width>        
        <font>2</font>
        <format>72</format>
        <priority>7</priority>
        <frequency>1000</frequency>
    </ssid_cap>	
	
    <key_cap>
        <enable>1</enable>
        <sx>1</sx>
        <sy>93</sy>
        <height>31</height>
        <width>37</width>        
        <font>2</font>
        <format>72</format>
        <priority>7</priority>
        <frequency>1000</frequency>
    </key_cap>	
    
    <!-- offload_ssid -->
    <offload_ssid>
        <enable>1</enable>
        <sx>0</sx>
        <sy>86</sy>
        <height>22</height>
        <width>128</width>
        <priority>4</priority>
        <font>2</font>
        <format>12</format>
        <frequency>1000</frequency>
    </offload_ssid>
    <!-- downloading -->
    <downloading>
        <enable>1</enable>
        <sx>0</sx>
        <sy>58</sy>
        <height>32</height>
        <width>128</width>        
        <priority>13</priority>
        <font>0</font>
	<format>140</format>
	<frequency>1000</frequency>
    </downloading>
   
    <!-- download_progress -->
    <download_progress>
        <enable>1</enable>
        <sx>0</sx>
        <sy>100</sy>
        <height>20</height>
        <width>128</width>
        <priority>13</priority>
        <font>2</font>
	<format>140</format>
    </download_progress>
		
    <!-- update -->
    <update>
        <enable>1</enable>
        <sx>0</sx>
        <sy>58</sy>
        <height>64</height>
        <width>128</width>        
        <priority>13</priority>
        <cbtime>3000</cbtime>
        <font>0</font>
	<format>140</format>
	<frequency>500</frequency>
    </update>
		
    <!-- roaming -->
    <roaming>
        <enable>1</enable>
        <sx>0</sx>
        <sy>80</sy>
        <height>22</height>
        <width>128</width>
        <priority>2</priority>
        <font>2</font>
        <format>12</format>
        <frequency>500</frequency>
    </roaming>
    
    <!-- ID_DYNAMIC_DO_MAIN -->
    <infomation>
        <enable>1</enable>
        <sx>0</sx>
        <sy>64</sy>
        <height>64</height>
        <width>128</width>
        <priority>8</priority>
        <font>2</font>
        <format>256</format>
        <cbtime>10000</cbtime>
    </infomation>

	    <!-- ID_DYNAMIC_IDLE_SSID -->
    <idlessid_cap>
        <enable>1</enable>
        <sx>40</sx>
        <sy>77</sy>
        <height>20</height>
        <width>75</width>
        <font>2</font>
        <format>152</format>
        <priority>10</priority>
        <cbtime>25000</cbtime>
    </idlessid_cap>

    <!-- ID_DYNAMIC_IDLE_PWD-->
    <idlepwd_cap>
        <enable>1</enable>
        <sx>40</sx>
        <sy>97</sy>
        <height>20</height>
        <width>75</width>
        <font>2</font>
        <format>152</format>
        <priority>10</priority>
        <cbtime>25000</cbtime>
    </idlepwd_cap>

</config>
